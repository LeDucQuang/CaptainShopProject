<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Customers</servlet-name>
        <servlet-class>admincontroller.Customers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Orders</servlet-name>
        <servlet-class>admincontroller.Order</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin</servlet-name>
        <servlet-class>admincontroller.Admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Products</servlet-name>
        <servlet-class>admincontroller.Products</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckValidData</servlet-name>
        <servlet-class>servlet.ajax.CheckValidData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Warehouses</servlet-name>
        <servlet-class>admincontroller.Warehouses</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Carts</servlet-name>
        <servlet-class>front_end_controller.Carts</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Customers</servlet-name>
        <url-pattern>/admin/Customers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Warehouses</servlet-name>
        <url-pattern>/admin/Warehouses</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/admin/Admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Orders</servlet-name>
        <url-pattern>/admin/Orders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Products</servlet-name>
        <url-pattern>/admin/Products</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckValidData</servlet-name>
        <url-pattern>/CheckValidData</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ShoppingCart</servlet-name>
        <servlet-class>front_end_controller.ShoppingCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddToCart</servlet-name>
        <servlet-class>front_end_controller.AddToCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckOut</servlet-name>
        <servlet-class>front_end_controller.CheckOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductDetails</servlet-name>
        <servlet-class>front_end_controller.ProductDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCategory</servlet-name>
        <servlet-class>front_end_controller.ViewCategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>front_end_controller.Home</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/web/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShoppingCart</servlet-name>
        <url-pattern>/web/ShoppingCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddToCart</servlet-name>
        <url-pattern>/web/AddToCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckOut</servlet-name>
        <url-pattern>/web/CheckOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductDetails</servlet-name>
        <url-pattern>/web/ProductDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewCategory</servlet-name>
        <url-pattern>/web/ViewCategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Carts</servlet-name>
        <url-pattern>/Carts</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
